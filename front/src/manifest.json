{
  "package": "com.csmdd.qwardrobe",
  "name": "Qwardrobe",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "designWidth":"750",
    "logLevel": "debug"
  },
  "router": {
    "entry": "Recommend",
    "pages": {
      "Recommend": {
        "component": "index"
      },
      "MyWardrobe": {
        "component": "index"
      },
      "MyCollocation":{
        "component":"index"
      },
      "Import":{
        "component":"index"
      },
      "About": {
        "component": "index"
      }
    },
    "widgets": {
      "CardDemo": {
        "name": "CardDemo",
        "description": "快应用卡片展示",
        "component": "index",
        "path": "/CardDemo",
        "minPlatformVersion": 1032,
        "targetManufactorys": [
          "vivo",
          "xiaomi"
        ],
        "features": []
      }
    }
  },
  "display": {
    "statusBarTextStyle":"light",
    "titleBarBackgroundColor": "#FDC9C9",
    "titleBarTextColor": "#FFFFFF",
    "menu": true,
    "pages": {
      "Recommend": {
        "titleBarText": "今日推荐",
        "menu": false
      },
      "MyWardrobe": {
        "titleBarText": "我的衣柜",
        "menu": false
      },
      "MyCollocation": {
        "titleBarText": "我的穿搭",
        "menu": false
      },
      "Import": {
        "titleBarText": "拍照导入"
      },
      "About": {
        "menu": false
      }
    }
  }
}
